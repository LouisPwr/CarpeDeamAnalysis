        # PENGUIN Prodigal
        done_assembly_taxonomy_faa_penguin=expand(
            f'{config["rdir"]}/assembly-genes-eval/{{smp}}.{{assm_input_1}}-{{assm_input_2}}.genes.penguin.{{config}}.faa',
