            assm_input_1=config["assm_input_1"],
            assm_input_2=config["assm_input_2"],
            config=penguin_configs,
        ),

        # PENGUIN sense
        done_penguin_sense=expand(
            f'{config["rdir"]}/assembly-sense/{{smp}}.{{assm_input_1}}-{{assm_input_2}}.mmseqs.penguin.{{config}}/dummy.txt',
