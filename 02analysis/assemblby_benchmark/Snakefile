        # MEGAHIT metadmg
        done_megahit_metadmg=expand(
            f'{config["rdir"]}/assembly-metadmg/{{smp}}.{{assm_input_1}}-{{assm_input_2}}.metadmg.megahit.{{config}}.dfit.gz',
