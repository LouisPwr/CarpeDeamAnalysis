        # MEGAHIT minimap
        done_megahit_minimap_sorted_sam=expand(
            f'{config["rdir"]}/assembly-minimap/{{smp}}.{{assm_input_1}}-{{assm_input_2}}.minimap.megahit.{{config}}.sorted.sam',
