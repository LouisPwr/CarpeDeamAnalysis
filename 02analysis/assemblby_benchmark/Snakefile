        ),
        done_assembly_taxonomy_fna_carpedeam=expand(
            f'{config["rdir"]}/assembly-genes-eval/{{smp}}.{{assm_input_1}}-{{assm_input_2}}.genes.carpedeam.{{config}}.fna',
